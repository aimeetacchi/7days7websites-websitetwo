s","name":null,"original":{"line":3,"column":16},"generated":{"line":273,"column":20}},{"source":"scss/_footer.scss","name":null,"original":{"line":64,"column":2},"generated":{"line":274,"column":2}},{"source":"scss/_footer.scss","name":null,"original":{"line":65,"column":2},"generated":{"line":275,"column":2}},{"source":"scss/_footer.scss","name":null,"original":{"line":67,"column":2},"generated":{"line":277,"column":0}},{"source":"scss/_footer.scss","name":null,"original":{"line":68,"column":4},"generated":{"line":278,"column":2}},{"source":"scss/_vars.scss","name":null,"original":{"line":7,"column":14},"generated":{"line":278,"column":9}}],"sources":{"scss/styles.scss":"@import url('https://fonts.googleapis.com/css?family=DM+Serif+Display|Karla:400,700&display=swap');\n\n@import './vars';\n@import './base';\n@import './header';\n@import './elements';\n@import './footer';","scss/_base.scss":"// * =================\n// * Base Styles\n// * ==================== */\n\nhtml {\n  box-sizing: border-box;\n  overflow-x: hidden;\n}\n\n*, *:before, *:after {\n  box-sizing: inherit;\n}\n\nbody {\n  background-color: #fff;\n  color: $color-greyish;\n  font-size: $base-font-size;\n  font-family: $base-font-family;\n  margin: 0;\n  overflow-x: hidden;\n}\n\nh1, h2, h3, h4, h5 {\n  font-family: $default-header;\n  color: $color-dark;\n  font-weight: 700;\n}\n\nimg {\n  display: block;\n  width: 100%;\n}\n\na {\n  color: $color-light;\n  border: 0;\n  font-weight: 400;\n  text-decoration: none;\n}","scss/_vars.scss":"// Colours \n\n$color-primary: #2d2640; // Dark Violet\n$color-greyish: #8E8C8F;\n$color-greyish-blue: #95a9c6; //Grayish Blue\n$color-dark-grayish-violet: #837d87;\n$color-light: #fafafa; // very-light-gray\n$color-dark: #2b272f; //Very Dark Violet\n\n\n \n//Breakpoints\n$phablet: 720px;\n$tablet: 1024px;\n$largeScreens: 1200px;\n$extraLargeScreens: 1500px; \n\n\n// Default Page Width\n$pageWidth: 1111px;\n\n\n// FONT STYLES =====\n$default-header: 'DM Serif Display', serif;\n$base-font-family: 'Karla', sans-serif;\n$base-font-size: 62.5%; // this defines what  is.\n\n\n","scss/_header.scss":"// Header Styles\n\n.header {\n  display:flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 1rem;\n  height: 5rem; // 80px\n  max-width: $pageWidth;\n  margin: 0 auto;\n\n\n  .logo {\n    width: 115px;\n  }\n\n  .nav {\n    font-size: 1rem;\n    position: relative;\n\n    &__burger-menu {\n      width: 35px;\n      height: 35px;\n      position: relative;\n      \n      @media (min-width: $tablet) {\n        display: none;\n      }\n\n      .burger-menu,\n      .close-burger-menu {\n        position: absolute;\n        top: 0;\n        left: 0;\n       \n      }\n\n      .close-burger-menu {\n        display: none;\n      }\n    }\n\n    &__list-items {     \n      text-transform: uppercase;\n      display: none;\n      list-style: none;\n      margin: 0;\n\n      @media (min-width: $tablet) {\n        display: flex;\n      }\n      \n      li {\n        padding: .5rem;\n        border: 1px solid #fff;\n        letter-spacing: 2px;\n\n        &:hover {\n          color: $color-dark;\n          border: 1px solid $color-dark;\n        }\n      }\n    }\n  }\n\n}","scss/_elements.scss":"// ELEMENTS Blocks\n\n.btn {\n  border:1px solid $color-light;\n  padding: 1rem 2rem;\n  text-transform: uppercase;\n  font-size: 1rem;\n}\n\n.btn--white {\n  background-color: $color-light;\n}\n\n// HERO ====\n.hero-container {\n  background-color: $color-primary;\n  .hero-section {\n    padding: 0; // 104px\n    text-align: center;\n    display: flex;\n    flex-direction: column-reverse;\n\n    @media (min-width: $tablet) {\n      padding: 6.5rem 0 0; // 104px\n      max-width: $pageWidth;\n      margin: 0 auto;\n      display: grid;\n      grid-template-columns: repeat(2, 1fr);\n      grid-gap: 3.5rem;\n    }\n    \n    &__title {\n      color: $color-light;\n      font-size: 3.5rem;\n      line-height: 1;\n\n      @media (min-width: $tablet) {\n        font-size: 3.8rem;\n        max-width: 430px;\n        \n      }\n    }\n\n    &__content {\n      font-size: 1rem;\n      color: $color-light;\n      padding: 1.5rem 1.5rem 9rem;\n\n      @media (min-width: $tablet) {\n        padding: 0;\n\n        &:before {\n          content: \"\";\n          width: 9rem;\n          height: 1px;\n          position: absolute;\n          background: $color-light;\n        }\n      }\n\n      \n    }\n    &__text {\n      padding-bottom: 2rem;   \n    }\n\n    &__image {\n\n    }\n\n  }\n}\n\n\n// MAIN SECTION\n\nmain {\n  text-align: center;\n  h2 {\n    font-size: 3rem;\n  }\n  .services {\n    // border:1px solid red;\n    font-size: 1rem;\n\n    @media (min-width: $tablet) {\n      display: flex;\n      justify-content: space-between;\n    }\n\n    &__section {\n      padding: 2rem;\n\n      h3 {\n        font-size: 2rem;\n      }\n    }\n\n    &__icon {\n      width: 100px;\n      margin: 0 auto;\n    }\n  }\n\n\n  // FIND OUT MORE BOX ===\n  .find-out-more {\n    background-color: $color-primary;\n    height: 50vh;\n    width: 80%;\n    margin: 8rem auto;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    position: relative;\n    overflow: hidden;\n    \n\n    @media (min-width: $tablet) {\n      padding: 2rem;\n      justify-content: flex-start;\n      flex-direction: row;\n      height: auto;\n      width: 100%;\n      max-width: $pageWidth;\n      margin: 0 auto;\n    }\n\n    &__bg-img {\n      position: absolute;\n      top: 0;\n      width: 100%;\n      height: auto;\n      transform: translateX(160px);\n  \n      img {\n        width: 250px;\n      }\n    }  \n\n    &__content {\n      margin: 3rem auto;\n      padding: 3rem;\n      z-index: 1;\n    }\n\n    \n\n    &__title {\n      margin: 0 0 3rem;\n      font-size: 2.5rem;\n      color: $color-light;\n    }\n  }\n}\n\n\n","scss/_footer.scss":"// Footer Styles\n\n.footer {\n  padding: 8rem 0 0;\n  position: relative;\n  background-color: $color-light;\n\n  &__bg-img {\n    position: absolute;\n    top: 0;\n    width: 100%;\n    height: auto; \n  }  \n\n  .logo {\n    position: relative;\n    width: 150px;\n    margin: 0 auto;\n    z-index: 1;\n  }\n\n  .socials {\n      position: relative;\n      border-bottom: 1px solid rgba($color-greyish, 50%);\n      display: flex;\n      justify-content: center;\n      max-width: 90%;\n      margin: 0 auto;\n      z-index: 1;\n\n      &__icon {\n        width: 30px;\n        margin: 2rem .8rem;\n      }\n  }\n\n  &__nav-list {\n    text-align: center;\n      font-size: .9rem;\n      list-style: none;\n      margin: 2rem 0;\n      padding: 0;\n      color: $color-primary;\n      font-weight: 700;\n      text-transform: uppercase;\n\n      \n\n      li {\n        padding: .5rem 0;\n\n        &:first-child {\n          padding-bottom: 3rem;\n          color: $color-dark-grayish-violet;\n        }\n      }\n      \n      \n    }\n}\n\n.attribution {\n  background-color: $color-primary;\n  text-align: center;\n  padding: 1rem;\n\n  a {\n    color: $color-light;\n  }\n}"},"lineCount":null}},"error":null,"hash":"2b3bc5ac9593c824051f60d5f0bfcb0f","cacheData":{"env":{}}}